#lists concept@replacing strings

list=["apple","banana","amma","nanna","brother","sis","friends"]
print(list[4:])
list[3]="ba"
print(list)
list[1]=("ab","bc","cd")
print(list)
print(list[-3])
list2=["spider man","the last ship","game of throwns","james bomd 007"]
